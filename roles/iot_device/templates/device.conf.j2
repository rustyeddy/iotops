# IoT Device Configuration
# Generated by Ansible - Do not edit manually

[device]
name = {{ iot_device_name }}
type = {{ iot_device_type }}
enabled = {{ iot_device_enabled | lower }}

[network]
mqtt_broker = {{ iot_device_mqtt_broker }}
mqtt_port = {{ iot_device_mqtt_port }}
mqtt_qos = {{ iot_device_mqtt_qos }}

[data]
collection_interval = {{ iot_device_collection_interval }}
data_format = {{ iot_device_data_format }}

[paths]
install_path = {{ iot_device_install_path }}
config_path = {{ iot_device_config_path }}
log_path = {{ iot_device_log_path }}

[environment]
environment = {{ environment | default('development') }}
